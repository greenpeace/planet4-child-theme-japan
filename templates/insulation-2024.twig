<!DOCTYPE html>
<html {{ fn('language_attributes', 'html') }} data-base="{{ data_nav_bar.home_url }}">
<head>
	<meta charset="{{ site.charset }}">
	{% include 'blocks/title.twig' %}

	{% include 'blocks/meta_fields.twig' %}

	{% include 'blocks/google_tag_manager.twig' %}

	{% if hubspot_active %}
		{% include 'head/hubspot.twig' %}
	{% endif %}

	{% include 'blocks/p4_structured_data.twig' %}

	<link rel="pingback" href="{{ site.pingback_url }}">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
	<meta name="robots" content="max-snippet:-1, max-image-preview:large, max-video-preview:-1">
	<link rel="shortcut icon" type="image/ico" href="{{ theme.uri }}/favicon.ico"/>
	{% include 'head/google_fonts.twig' %}

	{% if hreflang %}
		<!-- hreflang metadata -->
		<link rel="alternate" hreflang="x-default" href="https://www.greenpeace.org/international/">
		{% for code, url in hreflang %}
			<link rel="alternate" hreflang="{{ code }}" href="{{ url }}">
		{% endfor %}
	{% endif %}

    



	{{ wp_head }}

	{% if preconnect_domains %}
		{% for domain in preconnect_domains %}
			<link rel="preconnect" href="{{ domain }}">
		{% endfor %}
	{% endif %}

	{% if custom_page_css %}
		<link rel="stylesheet" href="{{custom_page_css}}">
	{% endif %}
	<!--CSS for insulation-->
    <link href="/japan/wp-content/themes/planet4-child-theme-japan/assets/src/insulation-2024/common/css/default.min.css" rel="stylesheet" />
    <link href="/japan/wp-content/themes/planet4-child-theme-japan/assets/src/insulation-2024/common/css/unit_insulation.min.css" rel="stylesheet" />
	<!--//CSS for insulation-->
	<!--JS for insulation-->
    <!--jquery-->
    <script src="/japan/wp-content/themes/planet4-child-theme-japan/assets/src/insulation-2024/common/js/jquery-3.6.0.min.js"></script>
    <script src="/japan/wp-content/themes/planet4-child-theme-japan/assets/src/insulation-2024/common/js/jquery.easing.1.4.1.js"></script>
    <!--inview パララクス関連-->
    <script type="text/javascript" src="/japan/wp-content/themes/planet4-child-theme-japan/assets/src/insulation-2024/common/js/jquery.inview.min.js"></script>
    <link href="/japan/wp-content/themes/planet4-child-theme-japan/assets/src/insulation-2024/common/css/parallax.min.css" rel="stylesheet" />
    <script type="text/javascript" src="/japan/wp-content/themes/planet4-child-theme-japan/assets/src/insulation-2024/common/js/parallax.js"></script>
    <!--slick（共通）-->
    <script type="text/javascript" src="/japan/wp-content/themes/planet4-child-theme-japan/assets/src/insulation-2024/common/js/slick/slick.min.js"></script>
    <link href="/japan/wp-content/themes/planet4-child-theme-japan/assets/src/insulation-2024/common/js/slick/slick.css" rel="stylesheet" />
    <link href="/japan/wp-content/themes/planet4-child-theme-japan/assets/src/insulation-2024/common/js/slick/slick-theme.css" rel="stylesheet" />
    <!--js-->
    <script src="/japan/wp-content/themes/planet4-child-theme-japan/assets/src/insulation-2024/common/js/default.js"></script>
    <script src="/japan/wp-content/themes/planet4-child-theme-japan/assets/src/insulation-2024/common/js/unit_insulation.js"></script>
	<!--//JS for insulation-->

</head>


<body class="{{ custom_body_classes|default("") }}" data-nro="{{ site.link }}" data-post-type="{{ fn( 'get_post_type' ) }}">
	{% if google_tag_value %}
		<!-- Google Tag Manager (noscript) -->
		<noscript><iframe src="https://www.googletagmanager.com/ns.html?id={{ google_tag_value }}"
						  height="0" width="0" style="display:none;visibility:hidden"></iframe>
		</noscript>
		<!-- End Google Tag Manager (noscript) -->
	{% endif %}

	<ul class="skip-links" aria-hidden="true">
		<li><a href="#header">{{ __( 'Skip to Navigation', 'planet4-master-theme' ) }}</a></li>
		<li><a href="#content">{{ __( 'Skip to Content', 'planet4-master-theme' ) }}</a></li>
		<li><a href="#footer">{{ __( 'Skip to Footer', 'planet4-master-theme' ) }}</a></li>
	</ul>
	{% if custom_styles.nav_type == 'minimal' %}
		{% include 'navigation-bar_min.twig' with data_nav_bar %}
	{% else %}
		{% include 'navigation-bar.twig' with data_nav_bar %}
	{% endif %}
	{% block content %}
		{{ post.content|raw }}
	{% endblock %}
	{# この部分の記述はフッターの上に位置する #}
	{# //この部分の記述はフッターの上に位置する #}
	{% block footer %}
		{% include 'footer.twig' with { 'nav_type' : custom_styles.nav_type } %}

		{% block cookies %}
			{% include 'cookies.twig' %}
		{% endblock %}

		{{ function('wp_footer') }}
	{% endblock %}
	<script id="countries_script" type="application/json">{% include "countries.json" %}</script>
	{% if custom_page_js %}
		<script src="{{custom_page_js}}"></script>
	{% endif %}
</body>
</html>


